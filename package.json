{
  "name": "cards-optimizer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run pull-env && next dev --turbopack",
    "deploy": "git push && .husky/post-push",
    "pull-env": "tsx scripts/sync-env.ts",
    "push-env": "tsx scripts/push-env.ts",
    "dev:notunnel": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit",
    "type-check:production": "tsc --noEmit --project tsconfig.production.json",
    "quality:check": "npm run type-check && npm run lint:strict && npm run test -- --passWithNoTests",
    "quality:scan": "echo 'ðŸ” Running full codebase quality scan...' && npm run type-check:production && npm run lint:strict && npm run test -- --passWithNoTests && node scripts/validate-docs.mjs",
    "docs:validate": "node scripts/validate-docs.mjs",
    "docs:coverage": "node scripts/generate-traceability-report.mjs",
    "docs:scan": "npm run docs:validate && npm run docs:coverage",
    "folder:check": "node scripts/validate-folder-structure.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:integration": "ENABLE_REAL_PLAID_TESTS=true jest __tests__/integration",
    "test:vault": "ENABLE_VAULT_TESTS=true jest __tests__/integration/supabase-vault.test.ts",
    "test:vault-e2e": "tsx scripts/test-vault-e2e.ts",
    "test:smoke": "tsx scripts/smoke-test-production.ts",
    "test:lighthouse": "lhci autorun",
    "test:mobile": "open http://localhost:3000 && echo 'Testing mobile: Open DevTools (Cmd+Opt+I) > Toggle Device Toolbar (Cmd+Shift+M)'",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "ios:sync": "npx cap sync ios",
    "ios:open": "npx cap open ios",
    "ios:dev": "npm run ios:sync && npm run ios:open",
    "prepare": "husky"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.11.1",
    "@babel/runtime": "^7.26.10",
    "@capacitor/app": "^7.1.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/status-bar": "^7.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@jl0810/db-client": "^1.0.11",
    "@jl0810/email-templates": "^1.0.3",
    "@paralleldrive/cuid2": "^3.0.4",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@react-email/components": "^0.5.7",
    "@sentry/nextjs": "^10.32.1",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^24.0.3",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "framer-motion": "^12.23.24",
    "glob": "^11.1.0",
    "hono": "^4.10.3",
    "lucide-react": "^0.553.0",
    "next": "^16.0.7",
    "next-auth": "^5.0.0-beta.30",
    "nodemailer": "6.10.1",
    "pg": "^8.16.3",
    "plaid": "^39.1.0",
    "postcss": "^8.5.6",
    "postgres": "^3.4.7",
    "preact": "^10.28.1",
    "prettier": "^3.3.3",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-email": "^4.3.2",
    "react-hook-form": "^7.67.0",
    "react-plaid-link": "^4.1.1",
    "react-syntax-highlighter": "^16.1.0",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "svix": "^1.81.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "^1.2.0",
    "zod": "^4.1.13"
  },
  "overrides": {
    "nodemailer": "6.10.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "baseline-browser-mapping": "^2.8.32",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
